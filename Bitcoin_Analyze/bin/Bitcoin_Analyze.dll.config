    <section name="SubSonicService" type="SubSonic.SubSonicSection, SubSonic" allowDefinition="MachineToApplication" restartOnExternalChanges="true"/>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net"/>
  </configSections>

  <connectionStrings>

    <add name="Bitcoin_Notify" connectionString="Data Source=win-s78rlfgopk6\SQLExpress; Database=Bitcoin_Notify; Integrated Security=true;"/>
    <add name="Bitcoin_NotifyConnectionString" connectionString="Data Source=win-s78rlfgopk6\SQLExpress;Initial Catalog=Bitcoin_Notify;Integrated Security=True"/>

    <add name="RippleTrade" connectionString="Data Source=win-s78rlfgopk6\SQLExpress; Database=RippleTrade; Integrated Security=true;"/>
    <add name="RippleTradeConnectionString" connectionString="Data Source=win-s78rlfgopk6\SQLExpress;Initial Catalog=RippleTrade;Integrated Security=True"/>
